#include <iostream>
#include <string>
using namespace std;

int main() {
    string a;
    string b;
    cin>>a>>b;
    int len=a.size();
    int lenl=b.size();
    cout<<len<<" "<<lenl<<endl;
    string c=a+b;
    cout<<c<<endl;
    char x=a[0];
    a[0]=b[0];
    b[0]=x;
    cout<<a<<" "<<b<<endl;
    
    
  
    return 0;
}
